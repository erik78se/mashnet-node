#!/bin/sh -e

mn_address="$(snapctl get mashnet-node.address)"
mn_wsport="$(snapctl get mashnet-node.wsport)"
mn_name="$(snapctl get mashnet-node.name)"

if [ -z "$mn_address" ]
then
      echo "\$mn_address is empty"
      exit 1
fi

if [ -z "$mn_wsport" ]
then
      echo "\$mn_wsport is empty"
      exit 1 
fi

